{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "jsRules": {
    "no-empty": true
}
}
